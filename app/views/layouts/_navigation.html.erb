<div class="row navbar" id="nav1">
  <!-- Toggle for mobile navigation, targeting the <ul> -->
  <a class="toggle" gumby-trigger="#nav1 > ul" href="#"><i class="icon-menu"></i></a>

  <h1 class="two columns logo">
    <a class="push_two" href="/">
      <%= image_tag 'logo.jpg' %>
    </a>
  </h1>
  <ul class="ten columns">
    <% if user_signed_in? %>
    <li><%= link_to 'Create Lead', new_aux_leads_path %></li>
    <li>
      <a href="#">Settings<i class="icon-cog"></i></a>

      <div class="dropdown">
        <ul>
          <li><%= link_to 'Edit Account', edit_user_registration_path %></li>
          <li><%= link_to 'Salesforce App Info', edit_credentials_path %></li>
        </ul>
      </div>
    </li>
    <% end %>
    <ul class="<%= user_signed_in? ? 'push_four' : 'push_eight'%>">
      <%= render 'layouts/authentication_links' %>
    </ul>
  </ul>


</div>